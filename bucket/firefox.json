    "extract_dir": "core",
    "bin": "firefox.exe",
    "shortcuts": [
        [
            "firefox.exe",
            "Mozilla Firefox"
        ]
    ],
    "checkver": {
        "url": "https://www.mozilla.org/en-GB/firefox/all/",
        "re": "data-latest-firefox=\"([\\d.]+)\""
