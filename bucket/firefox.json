    "architecture": {
        "64bit": {
            "url": "https://download.mozilla.org/?product=firefox-101.0.1-SSL&os=win64&lang=en-GB#/firefox.7z",
            "hash": "a25ea3c1e5ec4bfb6308af28795791b9623d01edc2f3981875eebec45f6edd06"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-101.0.1-SSL&os=win&lang=en-GB#/firefox.7z",
            "hash": "0fd8c8232bff218912022fe9fe00ac5a22f2d10433de0225b3230b00daabdd5b"
