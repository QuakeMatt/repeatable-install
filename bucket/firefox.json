    "architecture": {
        "64bit": {
            "url": "https://download.mozilla.org/?product=firefox-102.0.1-SSL&os=win64&lang=en-GB#/firefox.7z",
            "hash": "fc9bd189f15e02ccb394a94b99f9ce353b8bc2682014750180fa1e54d7b2a080"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-102.0.1-SSL&os=win&lang=en-GB#/firefox.7z",
            "hash": "9e387b95a2ef757ad9f7ed026e735fd36920b3e33df21e3e607d09cc21e2b750"
        }
    },
    "extract_dir": "core",
    "bin": "firefox.exe",
    "shortcuts": [
        [
            "firefox.exe",
            "Mozilla Firefox"
        ]
    ],
    "checkver": {
        "url": "https://www.mozilla.org/en-GB/firefox/all/",
        "re": "data-latest-firefox=\"([\\d.]+)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win64&lang=en-GB#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win64/en-GB/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            },
            "32bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win&lang=en-GB#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win32/en-GB/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            }
        }
    }
}
