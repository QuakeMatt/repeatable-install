    "architecture": {
        "64bit": {
            "url": "https://download.mozilla.org/?product=firefox-95.0-SSL&os=win64&lang=en-GB#/firefox.7z",
            "hash": "4d8db487443431de9b05778b625e6b19ce42780c3406b42337d1466effabc13d"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-95.0-SSL&os=win&lang=en-GB#/firefox.7z",
            "hash": "7612cc8958fe54cb8ede9338375ffb1b81276794d9dc2de89a3611d4b25c82e4"
        }
    },
    "extract_dir": "core",
    "bin": "firefox.exe",
    "shortcuts": [
        [
            "firefox.exe",
            "Mozilla Firefox"
        ]
    ],
    "checkver": {
        "url": "https://www.mozilla.org/en-GB/firefox/all/",
        "re": "data-latest-firefox=\"([\\d.]+)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win64&lang=en-GB#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win64/en-GB/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            },
            "32bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win&lang=en-GB#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win32/en-GB/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            }
        }
    }
}
