            "hash": "7c77105af67bb8147b18411874fef3d36104f7282c625a5a073cecc777611f78"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-84.0.1-SSL&os=win&lang=en-GB#/firefox.7z",
