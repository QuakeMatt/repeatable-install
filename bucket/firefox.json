            "hash": "d4eb0ee7b0af087dae73a28b90a6d202dac6739e2a2f512c91a4323178068970"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-72.0-SSL&os=win&lang=en-GB#/firefox.7z",
            "hash": "1cff7792abf5bc78628106c735afed77757862966fbe9c54bc1768eecf718973"
