    "architecture": {
        "64bit": {
            "url": "https://download.mozilla.org/?product=firefox-111.0-SSL&os=win64&lang=en-GB#/firefox.7z",
            "hash": "e82ef5cc0f89b986ab5b0f3652123208fd867f3be92eb069f66e7b2d6dd48052"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-111.0-SSL&os=win&lang=en-GB#/firefox.7z",
            "hash": "43bbfb133711209d60ac9df61207edb46253c0dc18931c004f6e8bef0b485499"
        }
    },
    "extract_dir": "core",
    "bin": "firefox.exe",
    "shortcuts": [
        [
            "firefox.exe",
            "Mozilla Firefox"
        ]
    ],
    "checkver": {
        "url": "https://www.mozilla.org/en-GB/firefox/all/",
        "re": "data-latest-firefox=\"([\\d.]+)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win64&lang=en-GB#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win64/en-GB/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            },
            "32bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win&lang=en-GB#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win32/en-GB/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            }
        }
    }
}
