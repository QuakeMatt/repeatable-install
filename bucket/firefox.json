            "hash": "e22aa1d870b123e6fbb5ba02e192fddee66ffa4c36fd1b7121709ed0329ac98c"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-71.0-SSL&os=win&lang=en-GB#/firefox.7z",
