    "architecture": {
        "64bit": {
            "url": "https://download.mozilla.org/?product=firefox-69.0-SSL&os=win64&lang=en-GB#/firefox.7z",
            "hash": "89eeb8b99f5571a991bbed2f31003b2140aa2fe88d5c6746e29b062ffbf39f8f"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-69.0-SSL&os=win&lang=en-GB#/firefox.7z",
            "hash": "fbb7bd3d28df21e1c5892f2bc3a38f24bf269386228082b4c054fa58e5484aa3"
        }
    },
    "extract_dir": "core",
    "bin": "firefox.exe",
    "shortcuts": [
        [
            "firefox.exe",
            "Mozilla Firefox"
        ]
    ],
    "checkver": {
        "url": "https://www.mozilla.org/en-GB/firefox/all/",
        "re": "data-latest-firefox=\"([\\d.]+)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win64&lang=en-GB#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win64/en-GB/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            },
            "32bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win&lang=en-GB#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win32/en-GB/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            }
        }
    }
}
