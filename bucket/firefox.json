    "architecture": {
        "64bit": {
            "url": "https://download.mozilla.org/?product=firefox-68.0.1-SSL&os=win64&lang=en-GB#/firefox.7z",
            "hash": "e205cbfb863c79c7890460257608f27175be02f1ad83de35017cd0b5237ac112"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-68.0.1-SSL&os=win&lang=en-GB#/firefox.7z",
            "hash": "acfbe4040dca90ee4058a419e2112c5e68d3bac854fa86ba1e469fd231f3bb6d"
        }
    },
    "extract_dir": "core",
    "bin": "firefox.exe",
    "shortcuts": [
        [
            "firefox.exe",
            "Mozilla Firefox"
        ]
    ],
    "checkver": {
        "url": "https://www.mozilla.org/en-GB/firefox/all/",
        "re": "data-latest-firefox=\"([\\d.]+)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win64&lang=en-GB#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win64/en-GB/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            },
            "32bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win&lang=en-GB#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win32/en-GB/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            }
        }
    }
}
