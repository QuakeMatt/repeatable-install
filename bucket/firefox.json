    "architecture": {
        "64bit": {
            "url": "https://download.mozilla.org/?product=firefox-67.0-SSL&os=win64&lang=en-GB#/firefox.7z",
            "hash": "4388b82ba63644b83b1f016ca8614a85a0d471eb8af72428e33d0ca3f48e254b"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-67.0-SSL&os=win&lang=en-GB#/firefox.7z",
