{
    "description": "Popular open source web browser. Cross-platform and available in many languages.",
    "homepage": "https://www.mozilla.org/en-GB/firefox/new/",
    "version": "76.0.1",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://download.mozilla.org/?product=firefox-76.0.1-SSL&os=win64&lang=en-GB#/firefox.7z",
            "hash": "bbcc493b837d5b68e19a2789b14c73ab2a99d43186d88959d604556fa82b65d7"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-76.0.1-SSL&os=win&lang=en-GB#/firefox.7z",
            "hash": "c71efe52cead4ae9eeda96c1f7146f6c1a69a407235e82b49782d37f3d3dd404"
        }
    },
    "extract_dir": "core",
    "bin": "firefox.exe",
    "shortcuts": [
        [
            "firefox.exe",
            "Mozilla Firefox"
        ]
    ],
    "checkver": {
        "url": "https://www.mozilla.org/en-GB/firefox/all/",
        "re": "data-latest-firefox=\"([\\d.]+)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win64&lang=en-GB#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win64/en-GB/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            },
            "32bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win&lang=en-GB#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win32/en-GB/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            }
        }
    }
}
